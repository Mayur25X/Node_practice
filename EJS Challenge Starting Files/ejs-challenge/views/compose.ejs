<%-include("partials/header")-%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        
        <label for="exampleInputEmail1">Title</label>
        <input type="text" name="postTitle" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Post</label>
        <textarea name="postBody" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" cols="40" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%-include("partials/footer")-%>
